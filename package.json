    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
